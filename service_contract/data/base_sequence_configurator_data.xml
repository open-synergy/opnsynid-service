<openerp>
<data>
<record id="base_sequence_service_contract" model="base.sequence_configurator">
    <field name="model_id" ref="service_contract.model_service_contract" />
    <field
                name="sequence_field_id"
                search="[('model_id.model','=','service.contract'),('name','=','name')]"
            />
    <field
                name="fallback_sequence_id"
                ref="service_contract.sequence_service_contract"
            />
</record>

<record
            id="base_sequence_service_contract_line"
            model="base.sequence_configurator_line"
        >
    <field name="generator_id" ref="base_sequence_service_contract" />
    <field name="sequence" eval="1" />
    <field
                name="sequence_computation_code"
            >result = document.type_id.contract_sequence_id</field>
</record>
</data>
</openerp>
